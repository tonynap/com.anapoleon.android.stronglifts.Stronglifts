<?xml version="1.0" encoding="utf-8"?>
<ScrollView 
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
    xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:id="@+id/RelativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbars="vertical">

        <TextView
            android:id="@+id/Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Stalls and Deloads"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Title"
            android:layout_toLeftOf="@+id/textView1"
            android:layout_toRightOf="@+id/textView14"
            android:text="    Stalls    "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/Title"
            android:layout_below="@+id/Title"
            android:text=" Deloads "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/textView2"
            android:text="Large Text"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/squatStall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView2"
            android:layout_alignRight="@+id/textView2"
            android:layout_below="@+id/textView4"
            android:ems="10"
            android:inputType="numberDecimal" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/squatStall"
            android:layout_alignBottom="@+id/squatStall"
            android:layout_alignParentLeft="true"
            android:text="Squats"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/squatStall"
            android:text="Large Text"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/benchStall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/squatStall"
            android:layout_alignRight="@+id/squatStall"
            android:layout_below="@+id/textView5"
            android:ems="10"
            android:inputType="numberDecimal" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/benchStall"
            android:text="Large Text"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/benchStall"
            android:layout_alignBottom="@+id/benchStall"
            android:layout_alignParentLeft="true"
            android:text="Bench"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/rowStall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/benchStall"
            android:layout_alignRight="@+id/benchStall"
            android:layout_below="@+id/textView8"
            android:ems="10"
            android:inputType="numberDecimal" >
        </EditText>

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/rowStall"
            android:layout_alignBottom="@+id/rowStall"
            android:layout_alignParentLeft="true"
            android:text="Row"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/rowStall"
            android:text="Large Text"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/pressStall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rowStall"
            android:layout_alignRight="@+id/rowStall"
            android:layout_below="@+id/textView10"
            android:ems="10"
            android:inputType="numberDecimal" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/pressStall"
            android:text="Large Text"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/deadliftStall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/pressStall"
            android:layout_alignRight="@+id/pressStall"
            android:layout_below="@+id/textView11"
            android:ems="10"
            android:inputType="numberDecimal" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/pressStall"
            android:layout_alignBottom="@+id/pressStall"
            android:layout_alignParentLeft="true"
            android:text="Press"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/deadliftStall"
            android:layout_alignBottom="@+id/deadliftStall"
            android:layout_alignParentLeft="true"
            android:text="Deadlift"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/squatDeload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/squatStall"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignRight="@+id/textView3"
            android:ems="10"
            android:inputType="numberDecimal" />

        <EditText
            android:id="@+id/benchDeload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView8"
            android:layout_alignLeft="@+id/squatDeload"
            android:layout_alignRight="@+id/squatDeload"
            android:ems="10"
            android:inputType="numberDecimal" />

        <EditText
            android:id="@+id/rowDeload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/benchDeload"
            android:layout_alignRight="@+id/benchDeload"
            android:layout_below="@+id/textView8"
            android:ems="10"
            android:inputType="numberDecimal" />

        <EditText
            android:id="@+id/pressDeload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rowDeload"
            android:layout_alignRight="@+id/rowDeload"
            android:layout_below="@+id/textView10"
            android:ems="10"
            android:inputType="numberDecimal" />

        <EditText
            android:id="@+id/deadliftDeload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/pressDeload"
            android:layout_alignRight="@+id/pressDeload"
            android:layout_below="@+id/textView11"
            android:ems="10"
            android:inputType="numberDecimal" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Title"
            android:layout_toLeftOf="@+id/textView3"
            android:text="Small"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignTop="@+id/textView2"
            android:text="Medium T"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:visibility="invisible" />
    </RelativeLayout>

</ScrollView>